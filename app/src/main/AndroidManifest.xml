        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Promote"
        android:usesCleartextTraffic="true"
        tools:targetApi="31">
        <activity
            android:name=".mydomain.MyDomainActivity"
            android:exported="false" />
        <activity
            android:name=".ProfileScreen"
            android:exported="false" />
        <activity
            android:name=".AddLinksScreen"
            android:exported="false" />
        <activity
            android:name=".RegisterTest"
            android:exported="true" />
        <activity
            android:name=".TestingOtp.PhoneActivity"
            android:exported="true" />
        <activity
            android:name=".TestingOtp.OtpActivity"
            android:exported="false" />
        <activity
            android:name=".linkhit.LinkHitActivity"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".mylinkspage.MyLinksActivity"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".addlinks.HomeActivity"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".login.LoginActivity"
            android:exported="true">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>

        <service android:name=".addlinks.LinksService" />
    </application>

</manifest>